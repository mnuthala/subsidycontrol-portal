          <div class="govuk-grid-row">
            <div class="govuk-grid-column-two-thirds">
              <form action="/reviewdetails" method="post" novalidate>


                  <h1 class="govuk-heading-xl">Granting Details</h1>

      
                 <!-- *****************************************  -->
                 <!-- Get the Granting Authority Name from user  -->
                 <!-- *****************************************  -->

                 <div class="govuk-form-group">
                    <label class="govuk-label govuk-label--m" for="Granting_Authority_Name">
                        Granting Authority Name
                    </label>

                    <input class="govuk-input govuk-input--width-20" id="Granting_Authority_Name" name="Granting_Authority_Name" type="text" aria-describedby="Granting_Authority_Name-hint">
                  </div>

                 <!-- *****************************************  -->
                 <!-- Get the Legal Granting Date     from user  -->
                 <!-- *****************************************  -->
      
                  <div class="govuk-form-group">
                    <fieldset class="govuk-fieldset" role="group" aria-describedby="Legal_Granting_Date-hint">
                      <legend class="govuk-fieldset__legend govuk-fieldset__legend--l">
                        <h1 class="govuk-label govuk-label--m">
                            Legal Granting Date
                        </h1>
                      </legend>
                      <div id="Legal_Granting_Date-hint" class="govuk-hint">
                        For example, 31 3 1980
                      </div>
                      <div class="govuk-date-input" id="Legal_Granting_Date">
                        <div class="govuk-date-input__item">
                          <div class="govuk-form-group">
                            <label class="govuk-label govuk-date-input__label" for="Legal_Granting_Date_Day">
                              Day
                            </label>
                            <input class="govuk-input govuk-date-input__input govuk-input--width-2" id="Legal_Granting_Date_Day" name="Legal_Granting_Date_Day" type="text" pattern="[0-9]*" inputmode="numeric">
                          </div>
                        </div>
                        <div class="govuk-date-input__item">
                          <div class="govuk-form-group">
                            <label class="govuk-label govuk-date-input__label" for="Legal_Granting_Date_Month">
                              Month
                            </label>
                            <input class="govuk-input govuk-date-input__input govuk-input--width-2" id="Legal_Granting_Date_Month" name="Legal_Granting_Date_Month" type="text" pattern="[0-9]*" inputmode="numeric">
                          </div>
                        </div>
                        <div class="govuk-date-input__item">
                          <div class="govuk-form-group">
                            <label class="govuk-label govuk-date-input__label" for="Legal_Granting_Date_Year">
                              Year
                            </label>
                            <input class="govuk-input govuk-date-input__input govuk-input--width-4" id="Legal_Granting_Date_Year" name="Legal_Granting_Date_Year" type="text" pattern="[0-9]*" inputmode="numeric">
                          </div>
                        </div>
                      </div>
                    </fieldset>
                  </div>


                 <!-- ************************************  -->
                 <!-- Get the Goods or Services  from user  -->
                 <!-- ************************************  -->

                 <div class="govuk-form-group">
                    <fieldset class="govuk-fieldset" aria-describedby="Goods_or_Services-hint">
                      <legend class="govuk-fieldset__legend govuk-fieldset__legend--l">
                        <h1 class="govuk-label govuk-label--m">
                          Goods or Services
                        </h1>
                      </legend>

                      <div class="govuk-radios govuk-radios--inline">
                        <div class="govuk-radios__item">
                          <input class="govuk-radios__input" id="Goods_or_Services" name="Goods_or_Services" type="radio" value="yes">
                          <label class="govuk-label govuk-radios__label" for="Goods_or_Services">
                            Yes
                          </label>
                        </div>
                        <div class="govuk-radios__item">
                          <input class="govuk-radios__input" id="Goods_or_Services-2" name="Goods_or_Services" type="radio" value="no">
                          <label class="govuk-label govuk-radios__label" for="Goods_or_Services-2">
                            No
                          </label>
                        </div>
                      </div>
                    </fieldset>
                  </div>

                 <!-- **********************************  -->
                 <!-- Get the Spending Region  from user  -->
                 <!-- **********************************  -->

                 <div class="govuk-form-group">
                    <label class="govuk-label govuk-label--m" for="Spending_Region">
                      Spending Region
                    </label>
                    <select class="govuk-select" id="Spending_Region" name="Spending_Region">
                      <option value="scotland">Scotland</option>
                      <option value="england" >England</option>
                      <option value="NorthernIreland">Northern Ireland</option>
                      <option value="Wales">Wales</option>
                    </select>
                  </div>


                <button class="govuk-button" data-module="govuk-button">
                  Review Details
                </button>
              </form>

              <form action="/grantingdetails" method="POST" >
                <div>
                <button class=" govuk-button_cancel_md govuk-button--secondary " >
